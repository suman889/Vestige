    "#for mathematical oppration need math library\n",
    "import math\n",
    "pi= math.pi\n",
    "'''Creat a function foe calculating Volume of sphere'''\n",
    "def Volume_of_sphere(r):\n",
    "    Vol =(4/3)*pi*r*r*r\n",
    "    return Vol\n",
    "\n",
    "\n",
    "radius = (12)\n",
    "#call the function\n",
    "print('Volume of sphere :' ,Volume_of_sphere(radius))"
